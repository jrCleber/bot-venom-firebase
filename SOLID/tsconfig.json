{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "resolveJsonModule": true,
    "outDir": "./src",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "removeComments": true,
    "allowJs": true
  },
  "include": ["./"]
}